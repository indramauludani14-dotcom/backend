{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "backend"
      }
    ]
  },
  "functions": {
    "source": ".",
    "runtime": "python311",
    "ignore": [
      "venv",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "__pycache__",
      "*.pyc",
      "*.pyo"
    ]
  }
}
